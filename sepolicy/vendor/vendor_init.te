# Allow vendor_init to write to sysfs_ssr_toggle
allow vendor_init sysfs_ssr_toggle:file w_file_perms;

typeattribute vendor_init data_between_core_and_vendor_violators;

allow vendor_init block_device:lnk_file setattr;

allow vendor_init fingerprint_data_file:dir create_file_perms;
allow vendor_init {
    tombstone_data_file
    preloads_data_file
    fingerprintd_data_file
    preloads_media_file
}:dir { create search getattr open read setattr ioctl write add_name remove_name rmdir relabelfrom };

allow vendor_init goodixfingerprintd_device:chr_file { getattr setattr };
set_prop(vendor_init, freq_prop)
