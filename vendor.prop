# Camera
camera.panorama.quality=high
persist.vendor.camera.privapp.list=com.android.cts.verifier,com.hmdglobal.camera2
vendor.camera.aux.packagelist=com.fihtdc.fap,com.evenwell.fqc,com.tools.alt,org.codeaurora.snapcam,com.hmdglobal.camera2

# CNE
persist.vendor.cne.feature=1

# Crypto
ro.crypto.volume.filenames_mode=aes-256-cts
ro.crypto.allow_encrypt_override=true
ro.crypto.set_dun=true

# Dalvik dex2oat
dalvik.vm.dex2oat64.enabled=true

# Display
ro.vendor.display.cabl=2
vendor.display.enable_default_color_mode=1
persist.sys.sf.native_mode=0

# DPM
persist.vendor.dpmhalservice.enable=1

# Factory reset protection
ro.frp.pst=/dev/block/bootdevice/by-name/frp

# FDP
ro.fdp.pst=/dev/block/bootdevice/by-name/fdp
ro.fdp.pst.offset=0x0000
ro.fdp.pst.size=0x1000

# FM
vendor.hw.fm.init=0

# Graphics
ro.opengles.version=196610
ro.hardware.vulkan=adreno
ro.hardware.egl=adreno

# IO cgroups
ro.vendor.iocgrp.config=1

# Misc
ro.panic.reboot.time=0
sys.autosuspend.timeout=500000
sys.vendor.shutdown.waittime=500

# Navigation
qemu.hw.mainkeys=0

# Netflix
ro.netflix.bsp_rev=Q845-05000-1

# Network manager
persist.vendor.data.iwlan.enable=true

# Perf
ro.vendor.extension_library=libqti-perfd-client.so

# Power
persist.vendor.qcomsysd.enabled=1
vendor.power.pasr.enabled=true

# Radio
ro.carrier=unknown
ro.telephony.iwlan_operation_mode=legacy
persist.vendor.radio.apm_sim_not_pwdn=1
persist.vendor.radio.sib16_support=1
persist.vendor.radio.custom_ecc=1
persist.vendor.radio.lte_vrte_ltd=1
persist.vendor.radio.procedure_bytes=SKIP
persist.radio.multisim.config=dsds
persist.vendor.radio.rat_on=combine
ro.telephony.default_network=33,33
ro.telephony.call_ring.multiple=false

# Sensors
persist.vendor.sensors.enable.mag_filter=true
persist.vendor.sensors.firmware_property_read_support=false

# USB
vendor.usb.diag.func.name=diag
vendor.usb.use_ffs_mtp=0
